@GDEF_Simple = [A A.smcp AE AE.smcp B B.smcp C C.smcp D D.smcp E E.smcp F F.smcp G G.smcp H H.smcp I I.smcp J J.smcp K K.smcp L L.smcp M M.salt M.salt.smcp M.smcp N N.salt N.salt.smcp N.smcp O O.smcp Oslash Oslash.smcp P P.smcp R R.smcp S S.smcp T T.smcp U U.smcp W W.salt W.salt.smcp W.smcp X X.salt X.salt.smcp X.smcp Y Y.smcp Z Z.smcp a a.salt a.superior ae c c.superior d d.salt d.superior dotlessi e e.superior g g.salt g.superior h i i.superior idotless jdotless k k.superior l l.superior m m.salt m.superior n n.salt n.superior o o.superior oslash p.salt r r.salt r.superior s s.superior t t.salt t.superior u u.salt u.superior w w.salt w.superior x.salt y y.superior z z.superior];
@GDEF_Ligature = [\ordmasculine \ordfeminine \fi \fl \b.superior \c.superior 
	\d.superior \e.superior \f.superior \g.superior \h.superior \i.superior 
	\j.superior \k.superior \l.superior \m.superior \n.superior \p.superior 
	\q.superior \r.superior \s.superior \t.superior \u.superior \v.superior 
	\w.superior \x.superior \y.superior \z.superior \onequarter \onehalf 
	\threequarters \f_f \f_f_i \ffl \c_t \s_t \t_t \f_t \t_y \f_y \ordmasculine.smcp 
	\ordfeminine.smcp \numero \c_k \c_h ];

table GDEF {
  GlyphClassDef @GDEF_Simple, @GDEF_Ligature, , ;

} GDEF;



