lookup dligDiscretionaryLigatureslookup14 {
  lookupflag 0;
    sub \f \f  by \f_f;
    sub \f \f \i  by \f_f_i;
    sub \f \f \l  by \ffl;
    sub \c \t  by \c_t;
    sub \s \t  by \s_t;
    sub \t \t  by \t_t;
    sub \f \t  by \f_t;
    sub \t \y  by \t_y;
    sub \f \y  by \f_y;
    sub \c \k  by \c_k;
    sub \c \h  by \c_h;
} dligDiscretionaryLigatureslookup14;

