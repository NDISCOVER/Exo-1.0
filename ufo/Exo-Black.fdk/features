#Mark attachment classes (defined in GDEF, used in lookupflags)

@GDEF_Simple = [A A.smcp AE AE.smcp B B.smcp C C.smcp D D.smcp E E.smcp F F.smcp G G.smcp H H.smcp I I.smcp J J.smcp K K.smcp L L.smcp M M.salt M.salt.smcp M.smcp N N.salt N.salt.smcp N.smcp O O.smcp Oslash Oslash.smcp P P.smcp R R.smcp S S.smcp T T.smcp U U.smcp W W.salt W.salt.smcp W.smcp X X.salt X.salt.smcp X.smcp Y Y.smcp Z Z.smcp a a.salt a.superior ae c c.superior d d.salt d.superior dotlessi e e.superior g g.salt g.superior h i i.superior idotless jdotless k k.superior l l.superior m m.salt m.superior n n.salt n.superior o o.superior oslash p.salt r r.salt r.superior s s.superior t t.salt t.superior u u.salt u.superior w w.salt w.superior x.salt y y.superior z z.superior];
@GDEF_Ligature = [\ordmasculine \ordfeminine \fi \fl \b.superior \c.superior 
	\d.superior \e.superior \f.superior \g.superior \h.superior \i.superior 
	\j.superior \k.superior \l.superior \m.superior \n.superior \p.superior 
	\q.superior \r.superior \s.superior \t.superior \u.superior \v.superior 
	\w.superior \x.superior \y.superior \z.superior \onequarter \onehalf 
	\threequarters \f_f \f_f_i \ffl \c_t \s_t \t_t \f_t \t_y \f_y \ordmasculine.smcp 
	\ordfeminine.smcp \numero \c_k \c_h ];

table GDEF {
  GlyphClassDef @GDEF_Simple, @GDEF_Ligature, , ;

} GDEF;



feature kern {
    @MMK_L_A = [A Aacute Abreve Abreveacute Abrevedotbelow Abrevegrave Abrevetilde Acircumflex Acircumflexacute Acircumflexdotbelow Acircumflexgrave Acircumflextilde Adieresis Adotbelow Agrave Ainvertedbreve Amacron Aogonek Aring Aringacute Atilde];
    @MMK_L_F = [F];
    @MMK_L_K = [K];
    @MMK_L_L = [L Lslash];
    @MMK_L_O = [D Dcroat Eth O Odieresismacron Odotaccentmacron Ohorn Oslash Otildemacron Q Schwa];
    @MMK_L_T = [T Tcommaaccent];
    @MMK_L_V = [V W W.salt];
    @MMK_L_Y = [Y];
    @MMK_R_A = [A AE Aacute Abreve Abreveacute Abrevedotbelow Abrevegrave Abrevetilde Acircumflex Acircumflexacute Acircumflexdotbelow Acircumflexgrave Acircumflextilde Adieresis Adotbelow Agrave Ainvertedbreve Amacron Aogonek Aring Aringacute Atilde];
    @MMK_R_O = [C Cacute Ccaron Ccedilla Ccircumflex Cdotaccent G Gbreve Gcaron Gcircumflex Gcommaaccent Gdotaccent O OE Oacute Obreve Ocircumflex Odblgrave Odieresis Odieresismacron Odotaccentmacron Odotbelow Ograve Ohorn Ohungarumlaut Oinvertedbreve Omacron Oogonek Oslash Oslashacute Otilde Otildemacron Q];
    @MMK_R_T = [T Tcommaaccent];
    @MMK_R_V = [V W W.salt];
    @MMK_R_X = [X X.salt];
    @MMK_R_Y = [Y];

    # glyph, group
    pos X @MMK_R_O -35;

    # group, group
    pos @MMK_L_A @MMK_R_O -15;
    pos @MMK_L_A @MMK_R_T -80;
    pos @MMK_L_A @MMK_R_V -70;
    pos @MMK_L_A @MMK_R_Y -75;
    pos @MMK_L_F @MMK_R_A -50;
    pos @MMK_L_K @MMK_R_O -35;
    pos @MMK_L_L @MMK_R_O -10;
    pos @MMK_L_L @MMK_R_T -70;
    pos @MMK_L_L @MMK_R_V -50;
    pos @MMK_L_L @MMK_R_Y -70;
    pos @MMK_L_O @MMK_R_A -15;
    pos @MMK_L_O @MMK_R_T -10;
    pos @MMK_L_O @MMK_R_V -30;
    pos @MMK_L_O @MMK_R_X -50;
    pos @MMK_L_O @MMK_R_Y -35;
    pos @MMK_L_T @MMK_R_A -80;
    pos @MMK_L_T @MMK_R_O -10;
    pos @MMK_L_V @MMK_R_A -70;
    pos @MMK_L_V @MMK_R_O -20;
    pos @MMK_L_Y @MMK_R_A -75;
    pos @MMK_L_Y @MMK_R_O -35;
} kern;

