languagesystem DFLT dflt;
languagesystem latn dflt;
languagesystem latn TRK;


# GSUB 

lookup dligDiscretionaryLigatureslookup14 {
  lookupflag 0;
    sub \f \f  by \f_f;
    sub \f \f \i  by \f_f_i;
    sub \f \f \l  by \ffl;
    sub \c \t  by \c_t;
    sub \s \t  by \s_t;
    sub \t \t  by \t_t;
    sub \f \t  by \f_t;
    sub \t \y  by \t_y;
    sub \f \y  by \f_y;
    sub \c \k  by \c_k;
    sub \c \h  by \c_h;
} dligDiscretionaryLigatureslookup14;

lookup ligaStandardLigatureslookup15 {
  lookupflag 0;
    sub \f \i  by \fi;
    sub \f \l  by \fl;
} ligaStandardLigatureslookup15;

lookup fracDiagonalFractionslookup13 {
  lookupflag 0;
    sub \zero by \zero.numerator ;
    sub \one by \one.numerator ;
    sub \two by \two.numerator ;
    sub \three by \three.numerator ;
    sub \four by \four.numerator ;
    sub \five by \five.numerator ;
    sub \six by \six.numerator ;
    sub \seven by \seven.numerator ;
    sub \eight by \eight.numerator ;
    sub \nine by \nine.numerator ;
} fracDiagonalFractionslookup13;

lookup fracDiagonalFractionslookup12 {
  lookupflag 0;
    sub \slash by \fraction ;
} fracDiagonalFractionslookup12;

lookup fracDiagonalFractionslookup11 {
  lookupflag 0;
    sub \one \fraction \four  by \onequarter;
    sub \one \slash \four  by \onequarter;
    sub \one \fraction \two  by \onehalf;
    sub \one \slash \two  by \onehalf;
    sub \three \fraction \four  by \threequarters;
    sub \three \slash \four  by \threequarters;
} fracDiagonalFractionslookup11;

lookup fracDiagonalFractionslookup10 {
  lookupflag 0;
    sub [\slash \fraction \zero.denominator \one.denominator \two.denominator 
	\three.denominator \four.denominator \five.denominator 
	\six.denominator \seven.denominator \eight.denominator 
	\nine.denominator ] [\zero.numerator \one.numerator \two.numerator \three.numerator 
	\four.numerator \five.numerator \six.numerator \seven.numerator 
	\eight.numerator \nine.numerator ]'  by [\zero.denominator \one.denominator 
	\two.denominator \three.denominator \four.denominator \five.denominator 
	\six.denominator \seven.denominator \eight.denominator \nine.denominator ];
} fracDiagonalFractionslookup10;

